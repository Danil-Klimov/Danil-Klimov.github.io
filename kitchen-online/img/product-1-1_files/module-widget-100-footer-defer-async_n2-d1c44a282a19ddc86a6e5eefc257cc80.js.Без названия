/// BEGIN sites/all/modules/app/src/Plugins/widget/order/order-result/order-result-images/order-result-images.js ///
$(function(){

  $('.b-order-result-images__slides').slick({
    variableWidth: true,
    // adaptiveHeight:true,
    slidesToShow: 3,
    slidesToScroll: 1,
    arrows: true,
    infinite: true,   
    dots: false,

    responsive: [{
      breakpoint: 1200,
      settings: {
        slidesToShow: 3
      }
    }, {
      breakpoint: 960,
      settings: {
        slidesToShow: 2
      }
    }, {
      breakpoint: 480,
      settings: {
        slidesToShow: 1,
        dots: true,
      }
    }]

  }).fadeIn();
});
/// END sites/all/modules/app/src/Plugins/widget/order/order-result/order-result-images/order-result-images.js ///

